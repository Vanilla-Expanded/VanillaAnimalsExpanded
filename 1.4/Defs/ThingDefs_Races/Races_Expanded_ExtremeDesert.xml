	<!-- ======================= Rattlesnake =============================== -->
	<ThingDef ParentName="AnimalThingBase">
		<defName>AEXP_Rattlesnake</defName>
		<label>rattlesnake</label>
		<description>A highly venomous snake, recieving it's name from a rattle at the end of their tail. The sound generated sounds as a warning to any predators, however in case it doesn't work, rattlesnake's venom-fangs inflict extremely painful toxin.</description>
		<statBases>
			<MoveSpeed>3.3</MoveSpeed>
			<MarketValue>200</MarketValue>
			<ComfyTemperatureMin>0</ComfyTemperatureMin>
			<ComfyTemperatureMax>60</ComfyTemperatureMax>
		</statBases>
		<tools>
			<li>
				<label>venom-fangs</label>
				<capacities>
					<li>AEXP_RattleSnakeVenomousBite</li>
				</capacities>
				<power>12</power>
				<cooldownTime>2.4</cooldownTime>
				<linkedBodyPartsGroup>Mouth</linkedBodyPartsGroup>
				<surpriseAttack>
					<extraMeleeDamages>
						<li>
							<def>Stun</def>
							<amount>14</amount>
						</li>
					</extraMeleeDamages>
				</surpriseAttack>
			</li>
			<li>
				<label>head</label>
				<capacities>
					<li>Blunt</li>
				</capacities>
				<power>3</power>
				<cooldownTime>2</cooldownTime>
				<linkedBodyPartsGroup>HeadAttackTool</linkedBodyPartsGroup>
				<ensureLinkedBodyPartsGroupAlwaysUsable>true</ensureLinkedBodyPartsGroupAlwaysUsable>
				<chanceFactor>0.2</chanceFactor>
			</li>
		</tools>
		<comps>
			<li Class="CompProperties_EggLayer">
				<eggFertilizedDef>AEXP_EggRattlesnakeFertilized</eggFertilizedDef>
				<eggFertilizationCountMax>1</eggFertilizationCountMax>
				<eggLayIntervalDays>10.0</eggLayIntervalDays>
				<eggProgressUnfertilizedMax>0.5</eggProgressUnfertilizedMax>
				<eggCountRange>
					<min>1</min>
					<max>2</max>
				</eggCountRange>
			</li>
		</comps>
		<race>
			<body>Snake</body>
			<predator>true</predator>
			<maxPreyBodySize>0.45</maxPreyBodySize>
			<baseBodySize>0.25</baseBodySize>
			<baseHungerRate>0.07</baseHungerRate>
			<baseHealthScale>0.5</baseHealthScale>
			<foodType>CarnivoreAnimal, OvivoreAnimal</foodType>
			<leatherDef>Leather_Lizard</leatherDef>
			<useMeatFrom>Cobra</useMeatFrom>
			<wildness>0.75</wildness>
			<petness>0.05</petness>
			<manhunterOnTameFailChance>0.40</manhunterOnTameFailChance>
			<manhunterOnDamageChance>0.10</manhunterOnDamageChance>
			<wildBiomes>
				<Desert>0.4</Desert>
				<ExtremeDesert>1</ExtremeDesert>
			</wildBiomes>
			<nameOnTameChance>1</nameOnTameChance>
			<lifeExpectancy>20</lifeExpectancy>
			<lifeStageAges>
				<li>
					<def>AnimalBaby</def>
					<minAge>0</minAge>
				</li>
				<li>
					<def>AnimalJuvenile</def>
					<minAge>0.15</minAge>
				</li>
				<li>
					<def>AnimalAdult</def>
					<minAge>0.5</minAge>
					<soundWounded>Pawn_Cobra_Wounded</soundWounded>
					<soundDeath>Pawn_Cobra_Death</soundDeath>
					<soundCall>AEXP_Pawn_Rattlesnake_Call</soundCall>
					<soundAngry>AEXP_Pawn_Rattlesnake_Angry</soundAngry>
				</li>
			</lifeStageAges>
			<soundMeleeHitPawn>Pawn_Melee_SmallScratch_HitPawn</soundMeleeHitPawn>
			<soundMeleeHitBuilding>Pawn_Melee_SmallScratch_HitBuilding</soundMeleeHitBuilding>
			<soundMeleeMiss>Pawn_Melee_SmallScratch_Miss</soundMeleeMiss>
		</race>
		<tradeTags>
			<li>AnimalUncommon</li>
			<li>AnimalFighter</li>
		</tradeTags>
