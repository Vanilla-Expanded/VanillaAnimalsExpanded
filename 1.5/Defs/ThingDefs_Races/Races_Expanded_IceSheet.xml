<?xml version="1.0" encoding="utf-8" ?>
<Defs>

	<!-- ======================= Penguin =============================== -->
	<ThingDef ParentName="AnimalThingBase">
		<defName>AEXP_Penguin</defName>
		<label>penguin</label>
		<description>An aquatic, flightless bird, only found in arctic regions. Often seen in massive flocks, penguins are fierce when cornered, and can quickly overrun any hunter incompetent enough to get close to them.</description>
		<statBases>
			<MoveSpeed>2.1</MoveSpeed>
			<MarketValue>120</MarketValue>
			<ComfyTemperatureMin>-60</ComfyTemperatureMin>
			<ComfyTemperatureMax>20</ComfyTemperatureMax>
			<FilthRate>4</FilthRate>
		</statBases>
		<tools>
			<li>
				<label>claws</label>
				<capacities>
					<li>Scratch</li>
				</capacities>
				<power>4</power>
				<cooldownTime>1.5</cooldownTime>
				<linkedBodyPartsGroup>Feet</linkedBodyPartsGroup>
				<surpriseAttack>
					<extraMeleeDamages>
						<li>
							<def>Stun</def>
							<amount>14</amount>
						</li>
					</extraMeleeDamages>
				</surpriseAttack>
			</li>
			<li>
				<label>beak</label>
				<capacities>
					<li>Bite</li>
				</capacities>
				<power>8</power>
				<cooldownTime>2</cooldownTime>
				<linkedBodyPartsGroup>Beak</linkedBodyPartsGroup>
				<surpriseAttack>
					<extraMeleeDamages>
						<li>
							<def>Stun</def>
							<amount>14</amount>
						</li>
					</extraMeleeDamages>
				</surpriseAttack>
			</li>
			<li>
				<label>head</label>
				<capacities>
					<li>Blunt</li>
				</capacities>
				<power>6</power>
				<cooldownTime>2</cooldownTime>
				<linkedBodyPartsGroup>HeadAttackTool</linkedBodyPartsGroup>
				<ensureLinkedBodyPartsGroupAlwaysUsable>true</ensureLinkedBodyPartsGroupAlwaysUsable>
				<chanceFactor>0.2</chanceFactor>
			</li>
		</tools>
		<comps>
			<li Class="CompProperties_EggLayer">
				<eggFertilizedDef>AEXP_EggPenguinFertilized</eggFertilizedDef>
				<eggFertilizationCountMax>1</eggFertilizationCountMax>
				<eggLayIntervalDays>3.5</eggLayIntervalDays>
				<eggProgressUnfertilizedMax>0.5</eggProgressUnfertilizedMax>
				<eggCountRange>1</eggCountRange>
			</li>
			<li Class="AnimalBehaviours.CompProperties_DigWhenHungry">

				<customThingToDig>AEXP_RawFish</customThingToDig>
				<customAmountToDig>10</customAmountToDig>

			</li>
		</comps>
		<race>
			<body>Bird</body>
			<leatherDef>Leather_Bird</leatherDef>
			<gestationPeriodDays>24</gestationPeriodDays>
			<herdAnimal>true</herdAnimal>
			<baseBodySize>0.8</baseBodySize>
			<baseHealthScale>0.8</baseHealthScale>
			<baseHungerRate>0.10</baseHungerRate>
			<foodType>CarnivoreAnimal</foodType>
			<wildness>0.70</wildness>
			<lifeExpectancy>50</lifeExpectancy>
			<useMeatFrom>Cassowary</useMeatFrom>
			<manhunterOnTameFailChance>0.16</manhunterOnTameFailChance>
			<manhunterOnDamageChance>0.22</manhunterOnDamageChance>
			<wildBiomes>
				<SeaIce>0.3</SeaIce>
				<IceSheet>1</IceSheet>
			</wildBiomes>
			<lifeStageAges>
				<li>
					<def>AnimalBabyTiny</def>
					<minAge>0</minAge>
				</li>
				<li>
					<def>AnimalJuvenile</def>
					<minAge>0.22</minAge>
				</li>
				<li>
					<def>AnimalAdult</def>
					<minAge>0.45</minAge>
					<soundWounded>AEXP_Pawn_Penguin_Wounded</soundWounded>
					<soundDeath>AEXP_Pawn_Penguin_Death</soundDeath>
					<soundCall>AEXP_Pawn_Penguin_Call</soundCall>
					<soundAngry>AEXP_Pawn_Penguin_Angry</soundAngry>
				</li>
			</lifeStageAges>
			<soundMeleeHitPawn>Pawn_Melee_SmallScratch_HitPawn</soundMeleeHitPawn>
			<soundMeleeHitBuilding>Pawn_Melee_SmallScratch_HitBuilding</soundMeleeHitBuilding>
			<soundMeleeMiss>Pawn_Melee_SmallScratch_Miss</soundMeleeMiss>
		</race>		
		<tradeTags>
			<li>AnimalUncommon</li>
		</tradeTags>
	</ThingDef>

