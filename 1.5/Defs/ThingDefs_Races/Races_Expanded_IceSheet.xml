	<!-- ======================= Seal =============================== -->

	<ThingDef ParentName="AnimalThingBase">
		<defName>AEXP_Seal</defName>
		<label>seal</label>
		<description>A medium sized semi-aquatic mammal, which only comes ashore to eat, mate and escape from predators. Being an aggresive hunter, seals are capable of tackling much bigger prey and are known for cannibalism if the food is scarce.</description>
		<statBases>
			<MoveSpeed>2.2</MoveSpeed>
			<MarketValue>200</MarketValue>
			<ComfyTemperatureMin>-70</ComfyTemperatureMin>
			<ComfyTemperatureMax>20</ComfyTemperatureMax>
			<FilthRate>4</FilthRate>
		</statBases>
		<tools>
			<li>
				<label>left fore-flipper</label>
				<capacities>
					<li>Scratch</li>
				</capacities>
				<power>9.4</power>
				<cooldownTime>2</cooldownTime>
				<linkedBodyPartsGroup>AEXP_ForeFlipers</linkedBodyPartsGroup>
			</li>
			<li>
				<label>right fore-flipper</label>
				<capacities>
					<li>Scratch</li>
				</capacities>
				<power>9.4</power>
				<cooldownTime>2</cooldownTime>
				<linkedBodyPartsGroup>AEXP_ForeFlipers</linkedBodyPartsGroup>
			</li>
			<li>
				<capacities>
					<li>Bite</li>
				</capacities>
				<power>12.5</power>
				<cooldownTime>2</cooldownTime>
				<linkedBodyPartsGroup>Teeth</linkedBodyPartsGroup>
				<chanceFactor>0.7</chanceFactor>
			</li>
			<li>
				<label>head</label>
				<capacities>
					<li>Blunt</li>
				</capacities>
				<power>6</power>
				<cooldownTime>2</cooldownTime>
				<linkedBodyPartsGroup>HeadAttackTool</linkedBodyPartsGroup>
				<ensureLinkedBodyPartsGroupAlwaysUsable>true</ensureLinkedBodyPartsGroupAlwaysUsable>
				<chanceFactor>0.2</chanceFactor>
			</li>
		</tools>
		<race>
			<body>AEXP_SimplePinniped</body>
			<herdAnimal>false</herdAnimal>
			<predator>true</predator>
			<maxPreyBodySize>0.81</maxPreyBodySize>
			<baseBodySize>0.90</baseBodySize>
			<baseHungerRate>0.23</baseHungerRate>
			<baseHealthScale>0.7</baseHealthScale>
			<meatLabel>pinniped meat</meatLabel>
			<foodType>OmnivoreAnimal, OvivoreAnimal</foodType>
			<leatherDef>AEXP_Leather_Pinniped</leatherDef>
			<wildness>0.65</wildness>
			<petness>0.18</petness>
			<nuzzleMtbHours>120</nuzzleMtbHours>
			<gestationPeriodDays>6.66</gestationPeriodDays>
			<litterSizeCurve>
				<points>
					<li>(0.5, 0)</li>
					<li>(1, 1)</li>
					<li>(1.5, 1)</li>
					<li>(2, 0)</li>
				</points>
			</litterSizeCurve>
			<wildBiomes>
				<IceSheet>0.8</IceSheet>
			</wildBiomes>
			<lifeExpectancy>12</lifeExpectancy>
			<lifeStageAges>
				<li>
					<def>AnimalBaby</def>
					<minAge>0</minAge>
				</li>
				<li>
					<def>AnimalJuvenile</def>
					<minAge>0.2</minAge>
				</li>
				<li>
					<def>AnimalAdult</def>
					<minAge>0.4</minAge>
					<soundWounded>AEXP_Pawn_Seal_Wounded</soundWounded>
					<soundDeath>AEXP_Pawn_Seal_Death</soundDeath>
					<soundCall>AEXP_Pawn_Seal_Call</soundCall>
					<soundAngry>AEXP_Pawn_Seal_Angry</soundAngry>
				</li>
			</lifeStageAges>
			<soundMeleeHitPawn>Pawn_Melee_SmallScratch_HitPawn</soundMeleeHitPawn>
			<soundMeleeHitBuilding>Pawn_Melee_SmallScratch_HitBuilding</soundMeleeHitBuilding>
			<soundMeleeMiss>Pawn_Melee_SmallScratch_Miss</soundMeleeMiss>
		</race>
		<tradeTags>
			<li>AnimalUncommon</li>
		</tradeTags>
	</ThingDef>

