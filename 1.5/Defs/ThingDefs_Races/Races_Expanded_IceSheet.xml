					<li>(1.5, 1)</li>
					<li>(2, 0)</li>
				</points>
			</litterSizeCurve>
			<wildBiomes>
				<IceSheet>0.8</IceSheet>
			</wildBiomes>
			<lifeExpectancy>40</lifeExpectancy>
			<lifeStageAges>
				<li>
					<def>AnimalBaby</def>
					<minAge>0</minAge>
				</li>
				<li>
					<def>AnimalJuvenile</def>
					<minAge>0.2</minAge>
				</li>
				<li>
					<def>AnimalAdult</def>
					<minAge>0.4</minAge>
					<soundWounded>AEXP_Pawn_Walrus_Wounded</soundWounded>
					<soundDeath>AEXP_Pawn_Walrus_Death</soundDeath>
					<soundCall>AEXP_Pawn_Walrus_Call</soundCall>
					<soundAngry>AEXP_Pawn_Walrus_Angry</soundAngry>
				</li>
			</lifeStageAges>
			<soundMeleeHitPawn>Pawn_Melee_SmallScratch_HitPawn</soundMeleeHitPawn>
			<soundMeleeHitBuilding>Pawn_Melee_SmallScratch_HitBuilding</soundMeleeHitBuilding>
			<soundMeleeMiss>Pawn_Melee_SmallScratch_Miss</soundMeleeMiss>
