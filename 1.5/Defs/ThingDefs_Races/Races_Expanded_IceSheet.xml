	<!-- ======================= Walrus =============================== -->

	<ThingDef ParentName="AnimalThingBase">
		<defName>AEXP_Walrus</defName>
		<label>walrus</label>
		<description>A large marine animal only found in the arctic regions of the planet, a walrus has prominent tusks which are a valuable luxury good across the planet.</description>
		<statBases>
			<MoveSpeed>2.4</MoveSpeed>
			<MarketValue>550</MarketValue>
			<ComfyTemperatureMin>-70</ComfyTemperatureMin>
			<ComfyTemperatureMax>10</ComfyTemperatureMax>
			<FilthRate>8</FilthRate>
			<MeatAmount>55.55</MeatAmount>
		</statBases>
		<tools>
			<li>
				<capacities>
					<li>Bite</li>
				</capacities>
				<power>16.5</power>
				<cooldownTime>2</cooldownTime>
				<linkedBodyPartsGroup>Teeth</linkedBodyPartsGroup>
				<chanceFactor>0.7</chanceFactor>
			</li>
			<li>
				<label>head</label>
				<capacities>
					<li>Blunt</li>
				</capacities>
				<power>6</power>
				<cooldownTime>2</cooldownTime>
				<linkedBodyPartsGroup>HeadAttackTool</linkedBodyPartsGroup>
				<ensureLinkedBodyPartsGroupAlwaysUsable>true</ensureLinkedBodyPartsGroupAlwaysUsable>
				<chanceFactor>0.2</chanceFactor>
			</li>
		</tools>
		<race>
			<body>AEXP_TuskedPinniped</body>
			<herdAnimal>false</herdAnimal>
			<predator>true</predator>
			<maxPreyBodySize>3.4</maxPreyBodySize>
			<baseBodySize>3.6</baseBodySize>
			<baseHungerRate>0.23</baseHungerRate>
			<baseHealthScale>3.2</baseHealthScale>
			<foodType>CarnivoreAnimal</foodType>
			<useMeatFrom>AEXP_Seal</useMeatFrom>
			<leatherDef>AEXP_Leather_Pinniped</leatherDef>
			<wildness>0.85</wildness>
			<petness>0.05</petness>
			<nuzzleMtbHours>120</nuzzleMtbHours>
			<gestationPeriodDays>8</gestationPeriodDays>
			<litterSizeCurve>
