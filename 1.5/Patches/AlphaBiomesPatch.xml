<?xml version="1.0" encoding="utf-8" ?>
<Patch>
	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Alpha Biomes</li>
		</mods>

		<match Class="PatchOperationSequence">
			<operations>


			
			<li Class="PatchOperationAdd"> 
				<xpath>/Defs/ThingDef[defName="AEXP_RedPanda"]/race/wildBiomes</xpath>
				<value>
					<AB_FeraliskInfestedJungle>0.01</AB_FeraliskInfestedJungle>
					<AB_MycoticJungle>0.1</AB_MycoticJungle>
					<AB_OcularForest>0.01</AB_OcularForest>
					<AB_GelatinousSuperorganism>0.2</AB_GelatinousSuperorganism>
					
				</value>
			</li>
			
			
			
			<li Class="PatchOperationAdd"> 
				<xpath>/Defs/ThingDef[defName="AEXP_Badger"]/race/wildBiomes</xpath>
				<value>
					
					<AB_MycoticJungle>0.01</AB_MycoticJungle>
					<AB_OcularForest>0.07</AB_OcularForest>
					<AB_GelatinousSuperorganism>0.01</AB_GelatinousSuperorganism>
					
					
				</value>
			</li>
			
			<li Class="PatchOperationAdd"> 
				<xpath>/Defs/ThingDef[defName="AEXP_WildGoose"]/race/wildBiomes</xpath>
				<value>
					<AB_FeraliskInfestedJungle>0.01</AB_FeraliskInfestedJungle>
					<AB_MycoticJungle>0.1</AB_MycoticJungle>
					<AB_OcularForest>0.1</AB_OcularForest>
					<AB_GelatinousSuperorganism>0.2</AB_GelatinousSuperorganism>
					
				</value>
			</li>
			
			<li Class="PatchOperationAdd"> 
				<xpath>/Defs/ThingDef[defName="AEXP_Hedgehog"]/race/wildBiomes</xpath>
				<value>
					<AB_FeraliskInfestedJungle>0.1</AB_FeraliskInfestedJungle>
					<AB_MycoticJungle>0.1</AB_MycoticJungle>
					<AB_OcularForest>0.1</AB_OcularForest>
					<AB_GelatinousSuperorganism>0.1</AB_GelatinousSuperorganism>
					<AB_MechanoidIntrusion>0.1</AB_MechanoidIntrusion>

				</value>
			</li>
			
			
			
			
			
		</operations>
		</match>
	</Operation>
	
	
</Patch>