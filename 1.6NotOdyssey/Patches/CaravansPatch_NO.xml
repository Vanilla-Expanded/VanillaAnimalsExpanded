<?xml version="1.0" encoding="utf-8" ?>
<Patch>
	<Operation Class="PatchOperationSequence">
		<success>Always</success>
		<operations>
			



			<!-- Caravan animals -->

		
			
		

			<li Class="PatchOperationAdd"> 
				<xpath>/Defs/BiomeDef[defName="Tundra"]/allowedPackAnimals</xpath>
				<value>

					<li>AEXP_Muskox</li>
				
				</value>
			</li>
			
			<li Class="PatchOperationAdd"> 
				<xpath>/Defs/FactionDef[@Name="OutlanderFactionBase"]/pawnGroupMakers/li[kindDef="Trader"]/carriers</xpath>
				<value>

					<AEXP_Muskox>10</AEXP_Muskox>
					

				</value>
			</li>
			 <li Class="PatchOperationAdd"> 
				<xpath>/Defs/FactionDef[@Name="TribeBase"]/pawnGroupMakers/li[kindDef="Trader"]/carriers</xpath>
				<value>

					<AEXP_Muskox>10</AEXP_Muskox>

				</value>
			</li>
		





		</operations>
	</Operation>


</Patch>

